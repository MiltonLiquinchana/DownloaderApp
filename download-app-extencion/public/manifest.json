{
    "name": "Mi extensi√≥n",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "webNavigation",
        "webRequest"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
"content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["./js/Button.js"],
        "css": ["./css/Button.css"]
    }
],
    "background": {
        "service_worker": "./js/Background.js"
    }
}